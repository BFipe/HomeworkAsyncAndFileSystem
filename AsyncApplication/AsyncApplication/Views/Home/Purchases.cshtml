@model List<CombinedViewModel>
@{
    if (ViewData["Info"] != null)
    {
        <h1>@ViewData["Info"]</h1>
    }


    <table class="table">
            <thead>
                <tr> 
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Product name</th>
                    <th>Type</th>
                    <th>Cost</th>
                    <th>Purchasing date</th>
                </tr>
            </thead>
            <tbody>
             @foreach (var Combined in Model)
                 {
            <tr>
                <td>@Combined.FirstName</td>
                <td>@Combined.LastName</td>
                <td>@Combined.Email</td>
                <td>@Combined.ProductName</td>
                <td>@Combined.ProductType</td>
                <td>@Combined.ProductCost $</td>
                <td>@Combined.PurchasingDate</td>

            </tr>
                    }
            </tbody>
    </table>
}